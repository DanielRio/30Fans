@model IEnumerable<_30Fans.Web.Models.TeamPhotoCompareModel>

@{
    ViewBag.Title = "TeamsPictures";
}

<h2>TeamsPictures</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TeamName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TeamPicturePath)
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TeamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TeamPicturePath)
            </td>

        </tr>
    }

</table>
